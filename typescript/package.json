{
  "name": "agentwrap",
  "version": "0.1.0",
  "description": "Wrap agents, ship APIs - Turn agent CLIs into libraries and OpenAI-compatible servers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run build && npm run test",
    "publish:dry": "npm publish --dry-run",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "codex",
    "openai",
    "typescript",
    "sdk"
  ],
  "author": "dashi0",
  "license": "MIT",
  "homepage": "https://github.com/dashi0/agentwrap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashi0/agentwrap.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/dashi0/agentwrap/issues"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@openai/codex": "^0.77.0",
    "express": "^4.18.2",
    "fs-extra": "^11.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
